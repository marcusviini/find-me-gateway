{
    "name": "gateway",
    "version": "1.0.0",
    "description": "gateway",
    "main": "server.js",
    "author": "https://github.com/marcusviin",
    "license": "ISC",
    "scripts": {
        "copy:unix": "cp -r -u ./src/main/pb/* ./deploy/main/pb",
        "copy:windows": "xcopy /s /Y .\\src\\main\\pb\\* .\\deploy\\main\\pb\\",
        "dev": "nodemon src/main/server.js",
        "start:unix": "npm install && npm run deploy && npm run copy:unix && npm run prod && pm2 log",
        "start:windows": "npm install && npm run deploy && npm run copy:windows && npm run prod && pm2 log",
        "prod": "pm2 start deploy/main/server.js --name FIND-ME-GATEWAY",
        "reload": "pm2 reload server",
        "deploy": "sucrase ./src -d ./deploy --transforms imports",
        "test:cov": "yarn jest --passWithNoTests --silent --runInBand --detectOpenHandles -- --coverage -c jest-unit.config.js",
        "test:unit": "yarn jest --passWithNoTests --runInBand --detectOpenHandles -c jest-unit.config.js",
        "test:staged": "yarn jest --passWithNoTests --silent --runInBand --detectOpenHandle --findRelatedTests --silent -c jest-unit.config.js",
        "pretest:integration": "cross-env NODE_ENV=test yarn migrate",
        "test:integration": "cross-env NODE_ENV=test yarn jest --watch -c jest-integration-config.js --detectOpenHandles",
        "posttest:integration": "cross-env NODE_ENV=test yarn migration:undo",
        "pretest:repository": "cross-env NODE_ENV=test yarn migrate",
        "test:repository": "cross-env NODE_ENV=test yarn jest --watch -c jest-repository-config.js --detectOpenHandles",
        "posttest:repository": "cross-env NODE_ENV=test yarn migration:undo"
    },
    "dependencies": {
        "@babel/core": "^7.12.10",
        "@grpc/grpc-js": "^1.3.7",
        "@grpc/proto-loader": "^0.5.6",
        "@sentry/node": "^6.2.1",
        "@types/express": "^4.17.12",
        "@types/qrcode": "^1.4.0",
        "@types/speakeasy": "^2.0.5",
        "@types/swagger-ui-express": "^4.1.2",
        "aws-sdk": "^2.888.0",
        "bcryptjs": "^2.4.3",
        "bull": "^3.20.0",
        "connect-timeout": "^1.9.0",
        "cors": "^2.8.5",
        "date-fns": "^2.16.1",
        "dd-trace": "^1.3.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-ipfilter": "^1.2.0",
        "express-rate-limit": "^5.2.3",
        "grpc": "^1.24.5",
        "helmet": "^4.4.1",
        "is-port-reachable": "^3.0.0",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "multer": "^1.4.2",
        "multer-s3": "^2.9.0",
        "nodemon": "^2.0.7",
        "npm-run-all": "^4.1.5",
        "pg": "^8.5.1",
        "pg-hstore": "^2.3.3",
        "pm2": "^4.5.4",
        "prom-client": "^14.0.0",
        "qrcode": "^1.4.4",
        "response-time": "^2.3.2",
        "sequelize": "^6.4.0",
        "sequelize-cli": "^6.2.0",
        "speakeasy": "^2.0.0",
        "sqlite": "^4.0.19",
        "sucrase": "^3.17.0",
        "supertest": "^6.1.3",
        "swagger-ui-express": "^4.1.6",
        "twilio": "^3.57.0",
        "yup": "^0.32.8"
    },
    "devDependencies": {
        "@jest-mock/express": "^1.3.0",
        "@sucrase/jest-plugin": "^2.0.0",
        "@types/jest": "^26.0.20",
        "babel-jest": "^26.6.3",
        "cross-env": "^7.0.3",
        "eslint": "^7.18.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-prettier-config": "^1.0.1",
        "git-commit-msg-linter": "^3.0.0",
        "husky": "^4.3.8",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.3",
        "mock-req-res": "^1.2.0",
        "prettier": "^2.2.1",
        "sinon": "^9.2.4",
        "ts-jest": "^26.4.4"
    }
}
